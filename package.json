{
  "name": "satisfactory_planner",
  "version": "0.0.1",
  "description": "Quick Calculator for Satisfactory",
  "author": "",
  "license": "ISC",
  "scripts": {
      "build": "webpack",
      "start": "webpack-dev-server --hot --open --mode development",
      "test": "jest",
      "test-watch": "jest --watch"
  },
  "eslintConfig": {
      "extends": "react-app"
  },
  "browserslist": [
      ">0.2%",
      "not dead",
      "not ie <= 11",
      "not op_mini all"
  ],
  "jest": {
      "collectCoverage": true,
      "collectCoverageFrom": [
          "src/**/*.{js,jsx}",
          "!**/index.js",
          "!**/LogIn.js"
      ],
      "coverageThreshold": {
          "global": {
              "branches": 70,
              "functions": 80,
              "lines": 80,
              "statements": 80
          }
      },
      "moduleNameMapper": {
          "\\.(ico|svg)$": "<rootDir>/__mocks__/fileMock.js",
          "\\.(css)$": "<rootDir>/__mocks__/styleMock.js"
      }
  },
  "devDependencies": {
      "@babel/core": "^7.6.2",
      "@babel/preset-env": "^7.6.2",
      "@babel/preset-react": "^7.0.0",
      "babel-jest": "^24.9.0",
      "babel-loader": "^8.0.6",
      "clean-webpack-plugin": "^2.0.2",
      "css-loader": "^2.1.1",
      "enzyme": "^3.10.0",
      "file-loader": "^3.0.1",
      "html-webpack-plugin": "^5.5.0",
      "jest": "^24.9.0",
      "json-loader": "^0.5.7",
      "mini-css-extract-plugin": "^0.5.0",
      "react-hot-loader": "^4.12.14",
      "redux-mock-store": "^1.5.3",
      "style-loader": "^0.23.1",
      "svg-inline-loader": "^0.8.0",
      "webpack": "^4.41.0",
      "webpack-cli": "^3.3.12",
      "webpack-dev-server": "^3.8.1"
  },
  "dependencies": {
      "bootstrap": "^4.3.1",
      "react": "^16.9.0",
      "react-bootstrap": "^1.0.0-beta.12",
      "react-dom": "^16.9.0",
      "react-google-login": "^5.0.5",
      "react-inlinesvg": "^0.8.4",
      "react-redux": "^6.0.1",
      "react-tabs": "^3.0.0",
      "react-transition-group": "^4.3.0",
      "redux": "^4.0.4",
      "redux-thunk": "^2.3.0"
  }
}